{
  "singleQuote": true,
  "trailingComma": "all",
  "parser": "typescript",
  "importOrder": [
    "^[./]",
    "^@nestjs/(.*)$",
    "^@root/(.*)$",
    "^@data/(.*)$",
    "^@/(.*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": [
    "typescript",
    "classProperties",
    "[\"decorators-legacy\", { \"decoratorsBeforeExport\": true }]"
  ]
}
